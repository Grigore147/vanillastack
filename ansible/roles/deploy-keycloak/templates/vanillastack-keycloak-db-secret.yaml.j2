apiVersion: v1
kind: Secret
metadata:
  name: keycloak-db-secret
  namespace: {{keycloak.namespace}}
type: Opaque
stringData:
  POSTGRES_EXTERNAL_PORT: "5432"
  POSTGRES_EXTERNAL_ADDRESS: "harbor-postgresql.{{keycloak.namespace}}.svc"
  POSTGRES_DATABASE: "keycloak"
  POSTGRES_HOST: "harbor-postgresql.{{keycloak.namespace}}.svc"
  POSTGRES_PASSWORD: "{{postgrespassword.stdout}}"
  POSTGRES_USERNAME: "keycloak"
  POSTGRES_SUPERUSER: "true"
